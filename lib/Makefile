all:
	$(MAKE) -C master all
	$(MAKE) -C slave all
	$(MAKE) -C logger all
	$(MAKE) -C ecg all
	$(MAKE) -C common all

clean:
	$(MAKE) -C master clean
	$(MAKE) -C slave clean
	$(MAKE) -C logger clean
	$(MAKE) -C ecg clean
	$(MAKE) -C common clean

docs:
	$(MAKE) -C master docs
	$(MAKE) -C slave docs
	$(MAKE) -C logger docs
	$(MAKE) -C ecg docs
	$(MAKE) -C common docs
